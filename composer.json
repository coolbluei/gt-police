{
  "name": "drupal-composer/drupal-project",
  "description": "Project template for GT Drupal 8 projects with Composer",
  "type": "project",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "IC Webteam",
      "email": "webteam@gatech.edu",
      "role": "Makers of things like this"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "path",
      "url": "gt/gt_profile"
    },
    {
      "type": "path",
      "url": "gt/gt_theme"
    },
    {
      "type": "path",
      "url": "gt/gt_tools"
    },
    {
      "type": "path",
      "url": "gt/hg_reader"
    },
    {
      "type": "package",
      "package": {
        "name": "woocommerce/flexslider",
        "version": "2.7.2",
        "type": "drupal-library",
        "source": {
          "url": "https://github.com/woocommerce/FlexSlider.git",
          "type": "git",
          "reference": "2.7.2"
        }
      }
    }
  ],
  "require": {
    "php": ">=7.3",
    "composer/installers": "^1.9",
    "cweagans/composer-patches": "^1.7",
    "drupal/admin_toolbar": "^2.4",
    "drupal/backup_migrate": "^5.0.1",
    "drupal/cas": "^1.7",
    "drupal/core-composer-scaffold": "^9.0.0",
    "drupal/core-project-message": "^9.0.0",
    "drupal/core-recommended": "^9.0.0",
    "drupal/field_formatter_class": "^1.4",
    "drupal/field_group": "^3.1",
    "drupal/flexslider": "^2.0",
    "drupal/jquery_ui_accordion": "^1.1",
    "drupal/layout_builder_modal": "^1.1",
    "drupal/layout_builder_restrictions": "^2.7",
    "drupal/link_target": "^1.2",
    "drupal/menu_link_attributes": "^1.2",
    "drupal/migrate_plus": "^5.1",
    "drupal/migrate_tools": "^5.0",
    "drupal/migrate_upgrade": "^3.2",
    "drupal/paragraphs": "^1.12",
    "drupal/pathauto": "^1.8",
    "drupal/redirect": "^1.6",
    "drupal/social_media_links": "^2.7",
    "drupal/stage_file_proxy": "^1.1",
    "drupal/video_embed_field": "^2.4",
    "drush/drush": "^10.0",
    "gt/gt_profile": "*",
    "gt/gt_theme": "*",
    "gt/gt_tools": "*",
    "gt/hg_reader": "*",
    "platformsh/config-reader": "^2.4",
    "webflo/drupal-finder": "^1.0.0",
    "webmozart/path-util": "^2.3",
    "woocommerce/flexslider": "~2.0"
  },
  "conflict": {
      "drupal/drupal": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
      "sort-packages": true
  },
  "extra": {
    "patchLevel": {
      "drupal/core": "-p2"
    },
    "installer-paths": {
      "web/core": ["type:drupal-core"],
      "web/libraries/{$name}": ["type:drupal-library"],
      "web/modules/contrib/{$name}": ["type:drupal-module"],
      "web/profiles/contrib/{$name}": ["type:drupal-profile"],
      "web/themes/contrib/{$name}": ["type:drupal-theme"],
      "drush/Commands/{$name}": ["type:drupal-drush"]
    },
    "drupal-scaffold": {
      "locations": {
        "web-root": "web"
      },
      "file-mapping": {
        "[web-root]/.htaccess": {
          "mode": "replace",
          "path": "assets/htaccess",
          "overwrite": false
        }
      }
    }
  }
}
